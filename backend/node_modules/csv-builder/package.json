{
  "name": "csv-builder",
  "author": "Nick Pisacane <pisacanen@gmail.com>",
  "version": "1.0.1",
  "description": "Easily encode complex JSON objects to CSV with CsvBuilder's schema-like API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickpisacane/CsvBuilder"
  },
  "keywords": [
